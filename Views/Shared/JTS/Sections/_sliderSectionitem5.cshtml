@model greenEnergy.ViewModel.pageContentVM

@{
    if (ViewBag.from == "web")
    {

        string classname = ViewBag.classname;
        if (Model != null)
        {
            <div class="ms-section ms-table qodef-m-slide-content @classname" style="" data-header-skin="light">
                <div class="qodef-m-template-content">
                    <div data-elementor-type="section" data-elementor-id="4696" class="elementor elementor-4696">
                        <section class="elementor-section elementor-top-section elementor-element elementor-element-59ec5a68 elementor-section-full_width elementor-section-height-min-height elementor-section-content-middle elementor-section-height-default elementor-section-items-middle qodef-elementor-content-no" data-id="59ec5a68" data-element_type="section" data-settings="{&quot;background_background&quot;:&quot;classic&quot;}">
                            <div class="elementor-container elementor-column-gap-default">
                                <div style="width:100%" class="elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-2fb47e62" data-id="2fb47e62" data-element_type="column">
                                    <div class="elementor-widget-wrap elementor-element-populated">
                                        <section class="elementor-section elementor-inner-section elementor-element elementor-element-16fe2607 elementor-section-full_width elementor-section-height-default elementor-section-height-default qodef-elementor-content-no" data-id="16fe2607" data-element_type="section">
                                            <div style="width:100%" class="elementor-container elementor-column-gap-default">
                                                <div style="width:100%" class="elementor-column elementor-col-50 elementor-inner-column elementor-element elementor-element-3385c313" data-id="3385c313" data-element_type="column">
                                                    <div class="elementor-widget-wrap elementor-element-populated">
                                                        <div class="elementor-element elementor-element-36e4105f elementor-widget elementor-widget-manufaktursolutions_core_single_image" data-id="36e4105f" data-element_type="widget" data-widget_type="manufaktursolutions_core_single_image.default">
                                                            <div class="elementor-widget-container">
                                                                <div class="qodef-shortcode qodef-m  qodef-single-image qodef-layout--default  ">
                                                                    <div class="qodef-m-image">
                                                                        <img decoding="async" width="200" height="230" src="~/webAssets/jahan/industry/wp-content/uploads/2022/10/h4-img-11-2.png" class="attachment-full size-full" alt="c" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="elementor-element elementor-element-bd8a44a elementor-widget elementor-widget-manufaktursolutions_core_section_title" data-id="bd8a44a" data-element_type="widget" data-widget_type="manufaktursolutions_core_section_title.default">
                                                            <div class="elementor-widget-container">
                                                                <div class="qodef-shortcode qodef-m  qodef-section-title qodef-alignment--left   ">
                                                                    @if (@Model.dataList.Count > 0)
                                                                    {
                                                                        <h6 class="qodef-m-title dirRight" style="color: #FFFFFF">
                                                                            @Model.dataList[0].title
                                                                        </h6>
                                                                    }

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="elementor-element elementor-element-37f656d6 elementor-widget elementor-widget-manufaktursolutions_core_separator" data-id="37f656d6" data-element_type="widget" data-widget_type="manufaktursolutions_core_separator.default">
                                                            <div class="elementor-widget-container">
                                                                <div class="qodef-shortcode qodef-m  qodef-separator clear  ">
                                                                    <div class="qodef-m-line" style="border-color: #FFFFFF;border-style: solid;width: 100%;border-bottom-width: 1px"></div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        @if (@Model.dataList.Count > 1)
                                                        {
                                                            for (int i = 1; i < Model.dataList.Count; i++)
                                                            {
                                                                <div class="elementor-element elementor-element-bd8a44a elementor-widget elementor-widget-manufaktursolutions_core_section_title" data-id="bd8a44a" data-element_type="widget" data-widget_type="manufaktursolutions_core_section_title.default">
                                                                    <div class="elementor-widget-container">
                                                                        <div class="qodef-shortcode qodef-m  qodef-section-title qodef-alignment--left   ">
                                                                            <h6 class="qodef-m-title dirRight" style="color: #FFFFFF">
                                                                                @Model.dataList[i].title
                                                                            </h6>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            }
                                                        }



                                                    </div>
                                                </div>
                                            </div>
                                        </section>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </div>
        }

    }
    else
    {

    }
}

