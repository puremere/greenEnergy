@model greenEnergy.ViewModel.pageContentVM



@if (Model.dataList != null)
{
    foreach (var item in Model.dataList)
    {
        <div class="elementor-element elementor-element-cbb2aa7 elementor-widget elementor-widget-manufaktursolutions_core_banner" data-id="cbb2aa7" data-element_type="widget" data-widget_type="manufaktursolutions_core_banner.default">
            <div class="elementor-widget-container">
                <div class="qodef-shortcode qodef-m  qodef-banner qodef-layout--link-button qodef-skin--light qodef-enable-top-border--yes qodef-disable-line-break--no ">
                    <div class="qodef-m-image">
                        <img fetchpriority="high" width="1100" height="400" src="~/Images/@System.Configuration.ConfigurationManager.AppSettings["name"]/Uploads/@item.mediaURL" class="attachment-full size-full" alt="d" decoding="async" sizes="(max-width: 1100px) 100vw, 1100px" />
                    </div>
                    <div class="qodef-m-content">
                        <div class="qodef-m-content-inner">
                            <div class="qodef-m-title-wrapper">
                                <h4 class="qodef-m-title" style="margin-top: 2px">
                                    @item.title
                                </h4>
                            </div>
                            <div class="qodef-m-button">
                                <a class="qodef-shortcode qodef-m  qodef-button qodef-layout--textual  qodef-html--link" href="@item.URL" target="_self" style="margin: 7px 0px 12px 0px">	<span class="qodef-m-text">@item.title2</span>    <span class="qodef-m-button-arrow-holder">		<svg class="qodef-svg--button-arrow" xmlns="http://www.w3.org/2000/svg" width="17.65" height="13.1" viewBox="0 0 17.65 13.1"><path d="m11.75 13.1-1.275-1.275L14.1 8.2l.85-.575-.1-.3-1.025.125H0V5.625h13.825l1.025.15.1-.3-.85-.575-3.625-3.625L11.75 0l5.9 5.9v1.3Z" /><path d="m11.75 13.1-1.275-1.275L14.1 8.2l.85-.575-.1-.3-1.025.125H0V5.625h13.825l1.025.15.1-.3-.85-.575-3.625-3.625L11.75 0l5.9 5.9v1.3Z" /></svg>	</span></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
}

