
@model greenEnergy.ViewModel.pageContentVM
@using Newtonsoft.Json;
@using greenEnergy.ViewModel;
@{


     string baseImage = "/Images/" + @System.Configuration.ConfigurationManager.AppSettings["name"] + "/Uploads/";
    string userimage = Model.dataList.SingleOrDefault(x => x.title == "img_image").title2 == "" ? "/webAssets/green2/img/11.jpg" : baseImage + Model.dataList.SingleOrDefault(x => x.title.Contains("img_image")).title2;
    string html_title = Model.dataList.SingleOrDefault(x => x.title == "title").title2 == "" ? "slider title" : Model.dataList.SingleOrDefault(x => x.title == "title").title2;
    string html_description = Model.dataList.SingleOrDefault(x => x.title == "description").title2 == "" ? "slider title" : Model.dataList.SingleOrDefault(x => x.title == "description").title2;
    string faceBook = Model.dataList.SingleOrDefault(x => x.title == "faceBook").title2 == "" ? "slider title" : Model.dataList.SingleOrDefault(x => x.title == "faceBook").title2;
    string twitter = Model.dataList.SingleOrDefault(x => x.title == "twitter").title2 == "" ? "slider title" : Model.dataList.SingleOrDefault(x => x.title == "twitter").title2;
    string linkedIn = Model.dataList.SingleOrDefault(x => x.title == "linkedIn").title2 == "" ? "slider title" : Model.dataList.SingleOrDefault(x => x.title == "linkedIn").title2;
    string insta = Model.dataList.SingleOrDefault(x => x.title == "insta").title2 == "" ? "slider title" : Model.dataList.SingleOrDefault(x => x.title == "insta").title2;

}



<div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="00ms">
    <div class="team-seven__single">
        <div class="team-seven__single__image">
            <img src="@userimage" alt="insur">
        </div>
        <div class="team-seven__single__hover">
            <div class="team-seven__single__hover__iconwrap"><i class="fa fa-share-alt"></i></div>
            <div class="team-seven__single__social">
                <a href="@faceBook">
                    <i class="fab fa-facebook-f"></i>
                    <span class="sr-only">Facebook</span>
                </a>
                <a href="@twitter">
                    <i class="fab fa-twitter" aria-hidden="true"></i>
                    <span class="sr-only">Twitter</span>
                </a>
                <a href="@linkedIn">
                    <i class="fab fa-linkedin-in"></i>
                    <span class="sr-only">Linkedin</span>
                </a>
                <a href="@insta">
                    <i class="fab fa-instagram"></i>
                    <span class="sr-only">Instagram</span>
                </a>
            </div>
        </div>
        <div class="team-seven__single__content">
            <h3 class="team-seven__single__name"><a href="team-details.html">@html_title</a></h3>
            <span class="team-seven__single__designation">@html_description</span>
        </div>
    </div>
</div>