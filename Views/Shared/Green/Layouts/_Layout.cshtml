<script>
     $("#cookies-popup").click(function(){
         $(this).css("display","none")
    });
      
    $(".language-select").val('@ViewBag.lang');
    $(".language-select").change(function(){

        var id = $(this).val();

          $.ajax({
            url: "home/changeLang",
            data:{
              lang : id
            },
              success: function(result){
              window.location.href = id;

           }});
    });
</script>
