    string description = Model.dataList.SingleOrDefault(x => x.title == "description").title2 != "" ? Model.dataList.SingleOrDefault(x => x.title == "description").title2 : "قیمت طراحی وب‌سایت پزشکی در سوپکت بستگی به ویژگی‌ها و امکانات مورد نظر شما دارد. ما با ارائه بسته‌های مختلف، سعی داریم تا نیازهای متنوع پزشکان و کلینیک‌ها را برآورده کنیم. از طراحی‌های ساده تا پیچیده، تمام خدمات ما با کیفیت بالا و قیمت‌های مناسب ارائه می‌شود. همچنین، ما در سوپکت به شما این امکان را می‌دهیم که با مشاوره رایگان، بهترین گزینه را بر اساس بودجه و نیازهای خود انتخاب کنید. هدف ما این است که شما بتوانید با حداقل هزینه، یک وب‌سایت کارآمد و جذاب داشته باشید که به رشد و موفقیت کسب‌وکار شما کمک کند.";


}




@if (Request.Cookies["adminToken"] != null)
{
