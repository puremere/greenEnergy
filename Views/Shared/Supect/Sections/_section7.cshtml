
@model greenEnergy.ViewModel.pageContentVM
@using greenEnergy.ViewModel;
@{
    string title = Model.dataList.SingleOrDefault(x => x.title == "header").title2 != "" ? Model.dataList.SingleOrDefault(x => x.title == "header").title2 : "We shuhrigd suh eygete dgtr.";
    string header = Model.dataList.SingleOrDefault(x => x.title == "header").title2 != "" ? Model.dataList.SingleOrDefault(x => x.title == "header").title2 : "A Strong Team.";

   
}

@if (Request.Cookies["adminToken"] != null)
{
    @Html.Partial("/Views/Shared/" + @System.Configuration.ConfigurationManager.AppSettings["name"] + "/Layouts/_controllerModalParent.cshtml", Model)
}
<section class="grey-bg" id="screenshots">
    <div class="container">
        <div class="title center">
            <h2>@header</h2>
            <h4>@title</h4>
        </div>
        <div class="row mt-50">
            <div class="owl-carousel" data-options="{&quot;items&quot;: 4, &quot;margin&quot;: 50, &quot;autoplay&quot;: true, &quot;dots&quot;: true}">
                @Html.Partial("/Views/Shared/" + @System.Configuration.ConfigurationManager.AppSettings["name"] + "/Layouts/_childContent.cshtml", Model)


            </div>
        </div>
    </div>
</section>