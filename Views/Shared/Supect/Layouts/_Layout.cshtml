                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <form method="post" action="/panel/setLayoutPartData" enctype="multipart/form-data" style="direction:rtl; padding:20px">
                        @Html.AntiForgeryToken()
